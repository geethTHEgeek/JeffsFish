<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jfs.controller.CustomerReport">
   <children>
      <TableView fx:id="table" layoutX="79.0" layoutY="141.0" prefHeight="225.0" prefWidth="293.0">
        <columns>
          <TableColumn fx:id="receiptId" prefWidth="122.0" text="Receipt ID" />
          <TableColumn fx:id="totalPaid" prefWidth="90.0" text="Total Paid" />
            <TableColumn fx:id="date" text="Date" />
        </columns>
      </TableView>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="&lt;&lt;back" />
      <Label layoutX="198.0" layoutY="12.0" prefHeight="35.0" prefWidth="193.0" text="Customer Report" textFill="#0f0bdd">
         <font>
            <Font name="Times New Roman Bold Italic" size="24.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="83.0" prefHeight="21.0" prefWidth="107.0" text="Customer Id" />
      <TextField fx:id="customerId" layoutX="121.0" layoutY="78.0" promptText="Enter customer ID" />
      <Button layoutX="341.0" layoutY="78.0" mnemonicParsing="false" onAction="#runReport" text="Run Report" />
   </children>
</AnchorPane>
