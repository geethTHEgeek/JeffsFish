            <Font name="Times New Roman Bold Italic" size="24.0" />
         </font>
      </Label>
      <Label fx:id="userNameLabel" layoutX="93.0" layoutY="137.0" prefHeight="21.0" prefWidth="181.0" text="User Name" textFill="#252424" />
      <Label fx:id="passwordLabel" layoutX="93.0" layoutY="205.0" prefHeight="21.0" prefWidth="181.0" text="Password" textFill="#252424" />
      <TextField fx:id="userName" layoutX="190.0" layoutY="132.0" promptText="User name (e-mail)" />
      <PasswordField fx:id="password" layoutX="190.0" layoutY="200.0" promptText="Password" />
      <Button fx:id="" layoutX="220.0" layoutY="274.0" mnemonicParsing="false" onAction="#logInAction" prefHeight="31.0" prefWidth="127.0" text="Log In" />
      <Label layoutX="193.0" layoutY="346.0" prefHeight="21.0" prefWidth="181.0" text="Don't have an account?" />
