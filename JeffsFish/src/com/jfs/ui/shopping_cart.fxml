<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jfs.controller.ShoppingCart">
   <children>
      <TableView fx:id="table" layoutX="39.0" layoutY="58.0" prefHeight="235.0" prefWidth="428.0">
        <columns>
          <TableColumn fx:id="product" prefWidth="156.0" text="Product" />
          <TableColumn fx:id="size" prefWidth="53.0" text="Size" />
            <TableColumn fx:id="quantity" prefWidth="98.0" text="Quantity" />
            <TableColumn fx:id="price" minWidth="7.0" prefWidth="107.0" text="Unit Price" />
        </columns>
      </TableView>
      <Label layoutX="29.0" layoutY="312.0" prefHeight="54.0" prefWidth="112.0" text="Total">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label fx:id="totalLbl" layoutX="134.0" layoutY="325.0" prefHeight="30.0" prefWidth="96.0">
         <font>
            <Font size="18.0" />
         </font></Label>
      <Button layoutX="442.0" layoutY="313.0" mnemonicParsing="false" onAction="#buyNow" prefHeight="54.0" prefWidth="112.0" text="Buy Now" />
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="&lt;&lt;back" />
      <Label layoutX="235.0" layoutY="19.0" prefHeight="28.0" prefWidth="158.0" text="Shopping Cart" textFill="#3c06ff">
         <font>
            <Font name="Times New Roman Bold Italic" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
